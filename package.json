{
  "name": "express-backend-template",
  "version": "1.0.0",
  "description": "Express Backend Template with User and Source Management",
  "main": "build/src/server.js",
  "scripts": {
    "main": "npm run build && cross-env NODE_ENV=production node build/src/server.js",
    "start": "cross-env NODE_ENV=production node build/src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc && npm run postbuild",
    "postbuild": "cpy 'docs/**/*' build/docs && cpy 'src/**/*.yaml' build/src",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "validate:swagger": "ts-node docs/validate-swagger.ts"
  },
  "keywords": [
    "express",
    "backend",
    "template",
    "typescript",
    "api"
  ],
  "author": "Harshal",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/lib-storage": "^3.850.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.10",
    "bwip-js": "^4.6.0",
    "canvas": "^3.1.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jimp": "^1.6.0",
    "jsbarcode": "^3.11.6",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "knex": "^3.1.0",
    "mammoth": "^1.10.0",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "nunjucks": "^3.2.4",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.3",
    "qrcode": "^1.5.4",
    "qrcode-reader": "^1.0.4",
    "rate-limit-redis": "^4.2.2",
    "redis": "^5.8.0",
    "sharp": "^0.34.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0",
    "yargs": "^18.0.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/aws-sdk": "^0.0.42",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.0.10",
    "@types/nunjucks": "^3.2.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "supertest": "^7.1.4",
    "typescript-eslint": "^8.35.1"
  }
}
