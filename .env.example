
# JWT Secret
JWT_SECRET=your-super-secure-256-bit-secret-key-that-is-at-least-32-chars-long
PORT=8000

# Development Database Configuration (optional)
# DB_HOST=localhost
# DB_USER=postgres
# DB_PASSWORD=your-password
# DB_DATABASE=postgres
# SCHEMA_NAME=public

# Production/Hosted Database Configuration
DATABASE_URL="postgresql://your-db-user:your-db-password@your-host:your-port/your-db-name"

# Redis Connection (optional)
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# AWS Config using Supabase S3
AWS_ACCESS_KEY=your-aws-access-key
AWS_SECRET_KEY=your-aws-secret-key
AWS_BUCKET_NAME=your-bucket-name
AWS_REGION=your-region
AWS_ENDPOINT=https://your-project-id.supabase.co/storage/v1/s3

# Email Configuration
EMAIL_USER=your-email@example.com
EMAIL_PASSWORD=your-email-password

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
